{
  "name": "@agtm/nf-manager-web",
  "description": "Frontend of the nf-manager project",
  "private": false,
  "main": "./index.mjs",
  "scripts": {
    "publish-patch": "npm test && npm version patch && npm publish --access public && git push",
    "publish-minor": "npm test && npm version minor && npm publish --access public && git push",
    "publish-major": "npm test && npm version major && npm publish --access public && git push",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "update": "npm update @agtm/nuxt-layer-adminlte-primeface && npm list @agtm/nuxt-layer-adminlte-primeface",
    "lint:test": "npx eslint src",
    "lint": "npx eslint --fix src",
    "link": "npm link @agtm/nuxt-layer-adminlte-primeface @agtm/util",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-jest": "^27.4.0",
    "nuxt": "^3.7.3"
  },
  "dependencies": {
    "@agtm/nuxt-layer-adminlte-primeface": "^0.2.4",
    "cronstrue": "^2.31.0",
    "lodash": "^4.17.21",
    "primevue": "^3.32.2",
    "socket.io-client": "^4.7.2"
  },
  "version": "0.0.4"
}
